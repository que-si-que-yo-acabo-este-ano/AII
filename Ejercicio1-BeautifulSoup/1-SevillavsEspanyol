<script language="javaScript" type="text/javascript" src="//as01.epimg.net/js/comun/ll/lazyload.20160120.min.js"></script>
<script language="javaScript" type="text/javascript">
var idComentariosNoticia = '1503091802-48abb960aa68d30402b5c290df809831';
var scriptThreadEskup = document.createElement("script");
scriptThreadEskup.src='//as.com/ThreadeskupSimple?action=info&th=' + idComentariosNoticia + '&f=jsonp&rnd=' + Math.random();

document.body.appendChild(scriptThreadEskup)

function setNumMensajescomentarios(num) {
  posi = ['', 'superior_', 'inferior_'];
  $.each(posi, function(index, value) {
      elemento = document.getElementById(value + "num_comentarios_noticia_1503091802-48abb960aa68d30402b5c290df809831");
      if (elemento) {
        elemento.innerHTML = "<a href='#EnlaceComentarios'>"+num+"</a>";
      }
  });
  if (!num) {
    document.getElementById("num_cabecera_comentarios_noticia_1503091802-48abb960aa68d30402b5c290df809831").innerHTML = '<span class="numero">'+num+'</span> comentarios';
    document.getElementById("boton_numero_comentarios_1503091802-48abb960aa68d30402b5c290df809831").innerHTML = '<span class="icon-btn as-icon-comentario"></span>Mostrar <strong>' + num + '</strong> comentarios';
  }
  else{
    if(num > 1 || num == 0){
      document.getElementById("num_cabecera_comentarios_noticia_1503091802-48abb960aa68d30402b5c290df809831").innerHTML = '<span class="numero">'+num+'</span> comentarios';
      document.getElementById("boton_numero_comentarios_1503091802-48abb960aa68d30402b5c290df809831").innerHTML = '<span class="icon-btn as-icon-comentario"></span>Mostrar <strong>' + num + '</strong> comentarios';
    }else{
      document.getElementById("num_cabecera_comentarios_noticia_1503091802-48abb960aa68d30402b5c290df809831").innerHTML = '<span class="numero">'+num+'</span> comentario';
      document.getElementById("boton_numero_comentarios_1503091802-48abb960aa68d30402b5c290df809831").innerHTML = '<span class="icon-btn as-icon-comentario"></span>Mostrar <strong>' + num + '</strong> comentario';
    }
  }
}
function callbackThreadeskup(_hashTMP){
  var _numeroComentariosNoticia = -1;
  // Se obtiene el numero de mensajes del hilo
  if (typeof _hashTMP.perfilesHilos != "undefined" &&
     typeof _hashTMP.perfilesHilos['_' + idComentariosNoticia] != "undefined" &&
     typeof _hashTMP.perfilesHilos['_' + idComentariosNoticia].numero_mensajes != "undefined") {

     _numeroComentariosNoticia = _hashTMP.perfilesHilos['_' + idComentariosNoticia].numero_mensajes;
     setNumMensajescomentarios(_numeroComentariosNoticia);
     if(_numeroComentariosNoticia == 0){
        document.getElementById('boton_numero_comentarios_1503091802-48abb960aa68d30402b5c290df809831').style.display = "none";
             }
  }
}
</script>
