        if(debug) console.log("registered with channel-id: "+sdk.channel.id)
        if(tag_alertas_edicion != undefined) {
          sdk.channel.tags.set([tag_alertas_edicion])
        }
        if(debug) console.log(sdk.channel.tags.list)
      })
    } else if(sdk.canRegister) {
        if(debug) console.log("channel-id: "+sdk.channel.id)
        if(tag_alertas_edicion == undefined) {
          sdk.channel.tags.set([])
          if(debug) console.log("set no tag")
        } else if(!sdk.channel.tags.has(tag_alertas_edicion)) {
          sdk.channel.tags.set([tag_alertas_edicion])
          if(debug) console.log("cambio de tag")
        }
        if(debug) console.log(sdk.channel.tags.list)
    }
  }).catch(function(err) {
    if(debug) console.log(err)
  })
